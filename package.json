{
  "name": "matrix-ui-plugin-boilerplate-24",
  "version": "1.0.1",
  "description": " matrix-ui-plugin-boilerplate for the 2.4 tree",
  "repository": {
    "url": "https://github.com/MatrixRequirements/matrix-ui-plugin-boilerplate-24"
  },
  "targets": {
    "default": {
      "context": "browser",
      "publicUrl": "/static/js/",
      "distDir": "./dist"
    }
  },
  "scripts": {
    "pretty": "prettier --write src/",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit",
    "check:metadata": "ajv --spec=draft2020 validate -s resources/plugin-metadata.json -d resources/matrix.json --strict=false",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack",
    "package": "zip -j ./dist/${npm_package_name}.zip ./dist/*.js* && zip -j -u ./dist/${npm_package_name}.zip ./resources/matrix.json",
    "proxy": "export $(grep -v '^#' Proxy.env | xargs) && ./proxy.sh",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@types/bootstrap": "3.3.32",
    "@types/bootstrap-datepicker": "0.0.15",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.3",
    "@types/jquery": "^3.5.5",
    "@types/jqueryui": "1.11.31",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "ajv": "^6.12.6",
    "ajv-cli": "^5.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "matrix-requirements-sdk": "^2.4.0-rc.0",
    "matrixrequirements-type-declarations": "^2.4.0-rc.0",
    "prettier": "^3.0.1"
  }
}
